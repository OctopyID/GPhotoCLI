{
    "name": "octopyid/gphoto",
    "description": "Google Photos CLI",
    "keywords": [
        "google",
        "photos",
        "albums"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Supian M",
            "email": "supianidz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-bcmath": "*",
        "ext-fileinfo": "*",
        "ext-pcntl": "*",
        "google/photos-library": "^1.7",
        "laravel-zero/framework": "^12.0",
        "nunomaduro/termwind": "^2.3",
        "octopyid/laravel-inotify": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.1",
        "phpunit/phpunit": "^12.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "gphoto"
    ]
}
